# Development Environment Configuration
ENV=dev
DEBUG=true

# Domain Configuration
DOMAIN=localhost
DOMAIN_ALIASES=127.0.0.1

# Protocol Configuration
HTTP_PROTOCOL=http
HTTPS_PROTOCOL=https
WS_PROTOCOL=ws
WSS_PROTOCOL=wss

# Port Configuration
HTTP_PORT=80
HTTPS_PORT=443
WEBSOCKET_PORT=8080

# SSL Configuration
SSL_CERT_PATH=/etc/apache2/ssl/dev/localhost.crt
SSL_KEY_PATH=/etc/apache2/ssl/dev/localhost.key
SSL_CERT_SUBJECT="/C=RO/ST=Bucharest/L=Bucharest/O=Localhost/OU=IT/CN=localhost"

# WebSocket Configuration
WEBSOCKET_SSL=false
WEBSOCKET_HOST=0.0.0.0

# Apache Configuration
APACHE_CONFIG_FILE=dev.conf
APACHE_ERROR_LOG=localhost_ssl_error.log
APACHE_ACCESS_LOG=localhost_ssl_access.log

# Database Configuration (if needed)
DB_HOST=localhost
DB_NAME=estimator_dev
DB_USER=root
DB_PASS=

# Logging
LOG_LEVEL=debug
WEBSOCKET_LOG_FILE=websocket_dev.log
